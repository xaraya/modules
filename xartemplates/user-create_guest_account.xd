<xar:template file="application_top" type="module"/>
<br>
<FORM method="POST" name="create_account" id="post" action="#xarModURL('commerce','user',(FILENAME_CREATE_GUEST_ACCOUNT, '', 'SSL')#" onsubmit="return check_form(create_account);">
<input type="hidden" name="action" value="process">
{if $error!=''}<br>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td>{$error}</td>
  </tr>
</table>
{/if}
<br>
<table width="100%" border="0" cellpadding="0" cellspacing="0">
  <tr>
    <td class="main"><strong>{#title_personal#}</strong> </td>
    <td><div align="right"><font color="#FF0000" size="1" face="Verdana, Arial, Helvetica, sans-serif">{#text_must#}</font></div></td>
  </tr>
</table>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
{if $gender=='1'}
  <tr>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;" width="200">{#text_gender#}</td>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;">{#text_male#}
      {$INPUT_MALE} {#text_female#} {$INPUT_FEMALE}</td>
  </tr>
  {/if}
  <tr>
    <td class="main" >{#text_firstname#}</td>
    <td class="main" >{$INPUT_FIRSTNAME}</td>
  </tr>
  <tr>
    <td class="main">{#text_lastname#}</td>
    <td class="main">{$INPUT_LASTNAME}</td>
  </tr>
  {if $birthdate=='1'}
  <tr>
    <td class="main">{#text_birthdate#}</td>
    <td class="main">{$INPUT_DOB}</td>
  </tr>
  {/if}
  <tr>
    <td class="main">{#text_email#}</td>
    <td class="main">{$INPUT_EMAIL}</td>
  </tr>
</table>
{if $company=='1'}
<br>
<strong>{#title_company#}</strong>
<table width="100%" border="0" cellspacing="0" cellpadding="0">

  <tr>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;" width="200">{#text_company#}</td>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;">{$INPUT_COMPANY}</td>
  </tr>

</table>
{/if}
<br>
<strong>{#title_address#}</strong>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;" width="200">{#text_street#}</td>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;">{$INPUT_STREET}</td>
  </tr>
  {if $suburb=='1'}
  <tr>
    <td class="main">{#text_suburb#}</td>
    <td class="main">{$INPUT_SUBURB}</td>
  </tr>
  {/if}
  <tr>
    <td class="main">{#text_code#}</td>
    <td class="main">{$INPUT_CODE}</td>
  </tr>
  <tr>
    <td class="main">{#text_city#}</td>
    <td class="main">{$INPUT_CITY}</td>
  </tr>
  {if $state=='1'}
  <tr>
    <td class="main">{#text_state#}</td>
    <td class="main">{$INPUT_STATE}</td>
  </tr>
  {/if}
  <tr>
    <td class="main">{#text_country#}</td>
    <td class="main">{$SELECT_COUNTRY}</td>
  </tr>
</table>
<br>
<strong>{#title_contact#}</strong>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;" width="200">{#text_tel#}</td>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;">{$INPUT_TEL}</td>
  </tr>
  <tr>
    <td class="main" >{#text_fax#}</td>
    <td class="main" >{$INPUT_FAX}</td>
  </tr>
</table>
<br>
<strong>{#title_options#}</strong>
<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td class="main" style="border-top: 1px solid; border-color: #cccccc;">{#text_newsletter#}
      {$CHECKBOX_NEWSLETTER}</td>
  </tr>
</table>
<br>

<table width="100%" border="0" cellspacing="0" cellpadding="0">
  <tr>
    <td><div align="right">{$BUTTON_SUBMIT}</div></td>
  </tr>
</table>
</FORM>