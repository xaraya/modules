<xar:template xmlns:xar="http://xaraya.com/2004/blocklayout">
<xar:comment> License: GPL http://www.gnu.org/copyleft/gpl.html </xar:comment>
<br/>
<xar:if condition="isset($words) and count($words) gt 0">
<b><xar:mlstring>Keywords :</xar:mlstring></b>
<div style="margin-left: 1em; margin-right: 1em; text-align:left;">
<xar:foreach in="$words" value="$word">
    <li><a href="#$word['url']#">#$word['keyword']#</a></li>
</xar:foreach>
</div>
<br/>
</xar:if>



</xar:template>
